
    <nav >
      <ul id="for_desk" class="month_container">
        <li class="month" id="jan"><a href="/?month=1">1월</a></li>
        <li class="month" id="feb"><a href="/?month=2">2월</a></li>
        <li class="month" id="mar"><a href="/?month=3">3월</a></li>
        <li class="month" id="apr"><a href="/?month=4">4월</a></li>
        <li class="month" id="may"><a href="/?month=5">5월</a></li>
        <li class="month" id="jun"><a href="/?month=6">6월</a></li>
        <li class="month" id="jul"><a href="/?month=7">7월</a></li>
        <li class="month" id="aug"><a href="/?month=8">8월</a></li>
        <li class="month" id="sep"><a href="/?month=9">9월</a></li>
        <li class="month" id="oct"><a href="/?month=10">10월</a></li>
        <li class="month" id="nov"><a href="/?month=11">11월</a></li>
        <li class="month" id="dec"><a href="/?month=12">12월</a></li>
      </ul>
      <div id="for_phone">
        <%var dateObj = new Date();%>
        <%var nowMonth = dateObj.getUTCMonth() + 1; //months from 1-12%>
        <% if(selectedMonth ===undefined){%>
          
          <% selectedMonth=nowMonth%>
          <% linkBack = nowMonth-1 %>
          <% link = nowMonth+1%> 
         <%} %> 
        <% selectedMonth=parseInt(selectedMonth) %>
        <% var linkBack = selectedMonth-1 %> 
        <% var link = selectedMonth+1 %> 
        
        <% if(selectedMonth>12){%>
          <%selectedMonth=1%>
        <%}else if(selectedMonth<1){%>
          <%selectedMonth=12%>
        <%} %>
        
        <% if (linkBack==0){%>
          <%linkBack=12%>
        <%} %>
        <% if (link==13){%>
          <%link=1%>
        <%} %>
        
      <a href="/?month=<%= linkBack %>"><i class="fas fa-angle-left angle"></i></a>
      <span id="p_month"><%= selectedMonth %></span>
      <a href="/?month=<%= link %>"><i class="fas fa-angle-right angle"></i></a>
      </div>
    </nav>
    
